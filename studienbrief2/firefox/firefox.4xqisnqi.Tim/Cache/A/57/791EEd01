								function PosFooter() {
									$(".footer").hide();
									$(".footer").css({'position':'absolute'});
									var posX = $("#s4-titlerow").height() + $("#s4-statusbarcontainer").height() + $("#s4-mainarea").height();
									var winX=window.innerHeight
									|| document.documentElement.clientHeight
									|| document.body.clientHeight;
									winX = winX - $(".footer").height()-44-$("#s4-ribboncont").height();
									var posY = $("#s4-mainarea").width();
									var winY=window.innerWidth
									|| document.documentElement.clientWidth
									|| document.body.clientWidth;
									if (posY > (winY)) {$(".footer").css({'width':posY});winX = winX -17;} //17px entspricht der Breite der Scrollbalken
									else {$(".footer").css({'width':winY-17});} 
									if (posX > (winX)) {$(".footer").css({'top':posX});}
									else {$(".footer").css({'top':winX}); }
									//$(".footer").html("PosY" + posY + " / WinY:" + winY);
									//$(".footer").append("Position Top:" + $(".footer").css('top'));
									$(".footer").show();
								};
